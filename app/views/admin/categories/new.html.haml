.col-md-10
  .content-box-large
    .breadcrumb-topbar
      %ol.breadcrumb
        %li
          = link_to "首頁", admin_root_path
        %li
          = link_to "分類列表", admin_categories_path
        %li.active 新增分類
    .panel-heading
      .panel-title
        %h3 新增分類
    .panel-body
      = form_for [:admin, @category], html: { class: "form-horizontal" } do |f|
        = render partial: "form", locals: { f: f }